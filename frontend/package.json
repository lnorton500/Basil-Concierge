{
  "name": "ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "ajv-keywords": "^3.4.1",
    "apexcharts": "^3.15.0",
    "axios": "^0.19.1",
    "moment": "^2.24.0",
    "node-sass": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "react": "^16.12.0",
    "react-apexcharts": "^1.3.6",
    "react-dom": "^16.12.0",
    "react-moment": "^0.9.7",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.0",
    "react-text-loop": "^2.3.0",
    "react-waypoint": "^9.0.2",
    "request": "^2.88.0",
    "serve": "^11.3.0",
    "styled-components": "^5.0.0"
  },
  "scripts": {
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "build-css": "node-sass --include-path ./node_modules ./src/styles/scss -o src/styles/css",
    "watch-css": "npm run build-css && node-sass --include-path ./node_modules ./src/styles/scss -o src/styles/css --watch",
    "start-js": "react-scripts start",
    "build-js": "react-scripts build",
    "prod": "npm run build; serve -s build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
